{
    "name": "novuso/common",
    "description": "Common library for the Novuso Framework",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "psr/container": "1.*",
        "psr/http-message": "1.*",
        "psr/log": "1.*",
        "novuso/system": "0.5.*"
    },
    "require-dev": {
        "doctrine/collections": "~1.0",
        "fzaninotto/faker": "1.6.*",
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/promises": "~1.0",
        "guzzlehttp/psr7": "~1.0",
        "mikey179/vfsStream": "1.6.*",
        "mockery/mockery": "0.9.*",
        "monolog/monolog": "1.*",
        "phpunit/phpunit": "6.2.*",
        "pimple/pimple": "~3.0",
        "satooshi/php-coveralls": "1.0.*",
        "squizlabs/php_codesniffer": "3.0.*",
        "symfony/symfony": "~3.0",
        "twig/twig": "~2.0",
        "willdurand/negotiation": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\Common\\Adapter\\": "src/Adapter",
            "Novuso\\Common\\Application\\": "src/Application",
            "Novuso\\Common\\Domain\\": "src/Domain"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Novuso\\Test\\Common\\Adapter\\": "test/Adapter",
            "Novuso\\Test\\Common\\Application\\": "test/Application",
            "Novuso\\Test\\Common\\Domain\\": "test/Domain",
            "Novuso\\Test\\Common\\Resources\\": "test/Resources"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
