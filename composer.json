{
    "name": "novuso/common",
    "description": "Novuso Common library",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "psr/container": "1.*",
        "psr/http-message": "1.*",
        "psr/log": "1.*",
        "novuso/system": "1.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.0",
        "codedungeon/phpunit-result-printer": "0.*",
        "consolidation/robo": "^1.0",
        "mikey179/vfsstream": "^1.0",
        "mockery/mockery": "^1.1",
        "monolog/monolog": "^1.0",
        "phploc/phploc": "^5.0",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\Common\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Novuso\\Common\\Test\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
